Algoritmo "lacosRepeticao-contadorSalto"

Var
contador, salto,valor: inteiro

Inicio

      contador <- 0

   escreva("\nEscreva quantas vezes: ")
   leia(valor)
   escreva("\nEscreva quantos saltos: ")
   leia(salto)la
   enquanto (contador<=valor) faca
         escreval(contador)
         contador <- contador + salto
   fimenquanto
   escreva("terminou")
Fimalgoritmo